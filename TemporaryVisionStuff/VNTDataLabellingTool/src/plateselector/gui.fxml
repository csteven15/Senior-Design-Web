<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.CheckBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ListView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="anchorPane" prefHeight="768" prefWidth="1024" xmlns="http://javafx.com/javafx/8.0.172-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="plateselector.Controller">
   <children>
      <ImageView fx:id="imgImageView" layoutX="350.0" layoutY="50.0" onMouseDragged="#mouseDragUpdate" onMousePressed="#mousePressed" onMouseReleased="#mouseReleased" pickOnBounds="true" preserveRatio="true" />
      <ListView fx:id="lstImagesList" layoutX="18.0" layoutY="52.0" prefHeight="648.0" prefWidth="291.0" />
      <Label fx:id="lblImages" layoutX="18.0" layoutY="22.0" text="Images:" AnchorPane.topAnchor="22.0" />
      <Button fx:id="btnExportAllResults" layoutX="18.0" layoutY="704.0" mnemonicParsing="false" onAction="#onClickExportAllResults" prefHeight="26.0" prefWidth="291.0" text="Export All Results" />
      <Button fx:id="btnLoadInformationFromLabelsfolder" layoutX="18.0" layoutY="735.0" mnemonicParsing="false" onAction="#onClickLoadInformationFromLabels" prefHeight="26.0" prefWidth="291.0" text="Load Information From Labels Folder" />
      <TextField fx:id="txtTag" layoutX="376.0" style="-fx-font-size: 22;" AnchorPane.bottomAnchor="50.0" />
      <Label fx:id="lblTag" layoutX="327.0" text="Tag:" AnchorPane.bottomAnchor="65.0" />
      <Button fx:id="btnFlipImage" layoutX="327.0" mnemonicParsing="false" onAction="#onClickFlipImage" text="Flip Image" AnchorPane.bottomAnchor="10.0" />
      <CheckBox fx:id="chkIsFloridaPlate" layoutX="423.0" mnemonicParsing="false" onMouseClicked="#onCheckboxClicked" text="Is Florida Plate (1)" AnchorPane.bottomAnchor="10.0" />
      <CheckBox fx:id="chkIsNonStandardDesign" layoutX="590.0" mnemonicParsing="false" onMouseClicked="#onCheckboxClicked" text="Is Non Standard Design (2)" AnchorPane.bottomAnchor="10.0" />
      <CheckBox fx:id="chkIsEdgeInfoObscured" layoutX="821.0" mnemonicParsing="false" onMouseClicked="#onCheckboxClicked" text="Is Edge Info Obscured (3)" AnchorPane.bottomAnchor="10.0" />
      <CheckBox fx:id="chkDoesNotContainPlate" layoutX="821.0" mnemonicParsing="false" onMouseClicked="#onCheckboxClicked" text="Does Not Contain Plate (4)" AnchorPane.bottomAnchor="50.0" />
      <ImageView fx:id="imgPlateView" layoutX="775.0" layoutY="560.0" AnchorPane.rightAnchor="25.0" AnchorPane.bottomAnchor="50.0" pickOnBounds="true" preserveRatio="true" />
   </children>
</AnchorPane>
